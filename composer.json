{
    "name": "duan617/laravel-doudian",
    "description": "抖音小店开放平台SDK",
    "keywords": [
        "abbotton",
        "doudian",
        "抖音小店",
        "抖店开放平台",
        "今日特卖",
        "抖店",
        "jinritemai"
    ],
    "homepage": "https://github.com/duanfuxing/laravel-doudian",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "duan617",
            "email": "1243702438@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "7.*",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Abbotton\\DouDian\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abbotton\\DouDian\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abbotton\\DouDian\\DouDianServiceProvider"
            ],
            "aliases": {
                "DouDian": "Abbotton\\DouDian\\DouDianFacade"
            }
        }
    }
}
